"sleep_day_1": "res://src/data/dialogic/sleep_day_1.dtl"
}
layout/style_directory={
"": "res://src/data/dialogic/custom_syle.tres",
"custom_syle": "res://src/data/dialogic/custom_syle.tres"
}
layout/style_list=["res://src/data/dialogic/custom_syle.tres"]
layout/default_style="res://src/data/dialogic/custom_syle.tres"
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.1
animations/join_default_wait=false
animations/leave_default_length=0.5
animations/leave_default_wait=false
animations/cross_fade_default_length=0.5
choices/autofocus_first=false
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
audio/channel_defaults={
"": {
"audio_bus": "",
"fade_length": 0.0,
"loop": false,
"volume": 0.0
},
"music": {
"audio_bus": "",
"fade_length": 0.0,
"loop": true,
"volume": 0.0
}
}
glossary/default_case_sensitive=true
variables={
"apartment": "",
"day": 1,
"espiou": false,
"espiou_algo_creepy_dia_1": false,
"first_package_day": true,
"hint": "",
"is_creepy": false,
"real_content": "",
"weight": ""
}
timer/process_in_physics=false
layout/end_behaviour=0
text/input_action="dialogic_default_action"
animations/join_default="Instant In"
animations/leave_default="Instant Out"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="Godot-4-Jam-Template"

[editor]

naming/script_name_casing=2

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/dialogic/plugin.cfg", "res://addons/gdUnit4/plugin.cfg", "res://addons/guide/plugin.cfg", "res://addons/markdownlabel/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "red",
"res://exports/": "gray",
"res://src/": "orange",
"res://src/autoloads/": "yellow",
"res://src/game/": "green",
"res://src/input/": "teal",
"res://src/ui/": "blue",
"res://test/": "purple",
"res://test/ui/": "purple",
"res://test/ui_test/": "purple"
}

[gdunit4]

report/assert/verbose_warnings=false
report/godot/script_error=false
ui/toolbar/run_overall=true

[godot_resource_groups]

auto_rebuild=true


; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Godot-4-Jam-Template"
config/description="Godot 4 template project for game jams"
run/main_scene="res://src/main.tscn"
config/features=PackedStringArray("4.5")
boot_splash/bg_color=Color(0.0784314, 0.133333, 0.25098, 1)
boot_splash/show_image=false
config/icon="res://icon.png"

[autoload]

Settings="*res://src/autoloads/settings.gd"
Globals="*res://src/autoloads/globals.gd"
Utils="*res://src/autoloads/utils.gd"
GUIDE="*res://addons/guide/guide.gd"
GameState="*res://src/autoloads/game_state.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"
Elevator="*res://src/scripts/elevator.gd"

[debug]

gdscript/warnings/unused_parameter=0
gdscript/warnings/untyped_declaration=1
gdscript/warnings/inferred_declaration=1

[dialogic]

directories/dch_directory={
"Porteiro": "res://src/data/dialogic/Porteiro.dch"
}
directories/dtl_directory={
"choose_elevator_floor": "res://src/data/dialogic/choose_elevator_floor.dtl",
"delivery_end_day_1": "res://src/data/dialogic/delivery_end_day_1.dtl",
"delivery_failure": "res://src/data/dialogic/delivery_failure.dtl",
"delivery_success_101": "res://src/data/dialogic/delivery_success_101.dtl",
"delivery_success_202": "res://src/data/dialogic/delivery_success_202.dtl",
"delivery_success_444": "res://src/data/dialogic/delivery_success_444.dtl",
"door_dialogue": "res://src/assets/facecards/door_dialogue.dtl",
"elevator_note": "res://src/data/dialogic/elevator_note.dtl",
"elevator_note_funcionarios": "res://src/data/dialogic/elevator_note_funcionarios.dtl",
"estatua": "res://src/data/dialogic/estatua.dtl",
"maquina": "res://src/data/dialogic/maquina.dtl",
"new_day_2": "res://src/data/dialogic/new_day_2.dtl",
